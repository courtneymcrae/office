<div class="panel-heading">
  <h3>Create Listing</h3> 
  </div>
  <div class="panel=body">
    <div class="container">

    <%= form_for @room, html: {multipart: true} do |f| %>

    <div class="row">
    <div class="col-md-4 select">
    <div class="form-group">
      <label>Property Type</label>
      <%= f.select :address, options_for_select([["Residnetial", "Residnetial"], ["Commercial", "Commercial"]]), :id => "address", :prompt => "Select...", :class => "form-control" %>

      <div>
        <%= f.label :address %>
        <%= f.text_field :address %>
      </div>

      <div>
        <%= f.label :price %>
        <%= f.text_field :price %>
      </div>

       <div>
        <%= f.label :summary %>
        <%= f.text_field :summary %>
      </div>
    </div>
    </div>
    </div>

    <div class="row">
      <div class="col-md-4">
        <div class="form-group">
          <span class="btn btn default btn-file">
            <i class = "fa fa-cloud-upload fa-lg"></i>Upload Photos 
            <%= file_field_tag "images[]", type: :file, multiple: true %>
          </span>
        </div>
      </div>
    </div>

  <div id="photos"><%=render 'photos/list' %></div>

  <%= f.submit "Create Listing", class: "btn btn-primary"%>
  <% end %>
  </div>
    </div>


